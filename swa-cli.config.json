{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "openai-chat": {
      "appLocation": ".",
      "apiLocation": "api",
      "outputLocation": "dist/openai-chat",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "npm run build --if-present",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:4200",
      "appName": "openai-chat",
      "resourceGroup": "openai-chat-rg",
      "useKeychain": false
    }
  }
}